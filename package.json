{
  "name": "yyl-env-pop-webpack-plugin",
  "version": "0.2.0",
  "engines": {
    "node": ">= 8.0.0"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "jest ./test/jest/",
    "c-base:watch": "node ./test/commander.js watch --path ./test/case/base/",
    "c-base:all": "node ./test/commander.js all --path ./test/case/base/",
    "cbw": "node ./test/commander.js watch --path ./test/case/base/",
    "cba": "node ./test/commander.js all --path ./test/case/base/"
  },
  "eslintConfig": {
    "env": {
      "jest": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended"
    ],
    "rules": {
      "semi": [
        "error",
        "never"
      ],
      "node/no-unpublished-require": [
        0
      ],
      "node/exports-style": [
        "error",
        "module.exports"
      ],
      "node/file-extension-in-import": [
        "error",
        "always"
      ],
      "node/prefer-global/buffer": [
        "error",
        "always"
      ],
      "node/prefer-global/console": [
        "error",
        "always"
      ],
      "node/prefer-global/process": [
        "error",
        "always"
      ],
      "node/prefer-global/url-search-params": [
        "error",
        "always"
      ],
      "node/prefer-global/url": [
        "error",
        "always"
      ],
      "node/prefer-promises/dns": "error",
      "node/prefer-promises/fs": 0,
      "quote-props": [
        "error",
        "consistent"
      ],
      "no-empty": [
        "error",
        {
          "allowEmptyCatch": true
        }
      ],
      "indent": [
        "warn",
        2
      ]
    }
  },
  "eslintIgnore": [
    "node_modules",
    "/test/case",
    "yyt",
    "__frag"
  ],
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.4.2",
    "webpack": "^4.41.1",
    "yyl-fs": "^0.3.3",
    "yyl-os": "^0.10.2",
    "yyl-util": "^2.3.8"
  },
  "peerDependencies": {
    "webpack": ">=4.0.0"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "tapable": "^1.1.3"
  },
  "description": "## USAGE ```javascript ```",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackness1208/yyl-env-pop-webpack-plugin.git"
  },
  "author": "jackness",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jackness1208/yyl-env-pop-webpack-plugin/issues"
  },
  "homepage": "https://github.com/jackness1208/yyl-env-pop-webpack-plugin#readme"
}
